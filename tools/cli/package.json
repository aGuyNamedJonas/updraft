{
  "name": "@updraft/cli",
  "description": "Easily get started with building your own updraft components (use init), download runnable examples for updraft components (use templates), or setup a private updraft component library (use doc & publish).",
  "version": "0.0.14",
  "author": "aGuyNamedJonas @aGuyNamedJonas",
  "bin": {
    "updraft": "./bin/run"
  },
  "bugs": "https://github.com/aGuyNamedJonas/updraft/issues",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@microsoft/tsdoc": "^0.12.20",
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@types/jest": "^26.0.3",
    "chalk": "^4.1.0",
    "cli-ux": "^5.4.6",
    "debug": "^4.1.1",
    "glob": "^7.1.6",
    "handlebars": "^4.7.6",
    "inquirer": "^7.2.0",
    "json-schema": "^0.2.5",
    "new-github-issue-url": "^0.2.1",
    "opn": "^6.0.0",
    "parse-diff": "^0.7.0",
    "tslib": "^1",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "babel-jest": "^26.1.0",
    "globby": "^10",
    "ts-node": "^8"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/aGuyNamedJonas/updraft",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "updraft",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "message": "New version available for @updraft/cli"
    }
  },
  "repository": "aGuyNamedJonas/updraft",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest --watch",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
